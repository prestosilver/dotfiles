-- color theme
funcs = require("conpositor.funcs")

-- generated with mondo
session:set_color(true, "border", "#ffffffFF")
session:set_color(true, "background", "#ffffffFF")
session:set_color(true, "foreground", "#f9f5d7FF")

session:set_color(false, "border", "#ffffffFF")
session:set_color(false, "background", "#ffffffFF")
session:set_color(false, "foreground", "#000000FF")

funcs.set_font_pt("Papyrus Pixel Mono", 14)
